package template;

public class Singleton1 {

	// 1st step ---> Same type attribute
	private static Singleton1 singletonWawita = null;

	// 2nd Private constructor

	private Singleton1() {

	}

	// Global access
	public static Singleton1 getSingletonWawtita() {
		if (singletonWawita != null) {
			singletonWawita = new Singleton1();
		}
		return singletonWawita;

	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
