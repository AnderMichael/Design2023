package exercises;

import template.SingletonWawita3;

public class LaunchStore {

	public static void main(String[] args) {
		
		Logger logger = Logger.getLogger();
		Thread user1 = new Thread(new Runnable() {
			@Override
			public void run() {
				Product product =  new Product("OLA", 192.00);
				logger.register(product.presentation());
			}
		});

		Thread user2 = new Thread(new Runnable() {
			@Override
			public void run() {
				Client client =  new Client("Andersaurio","Cayman","12" );
				logger.register(client.presentation());

			}
		});

		Thread user3 = new Thread(new Runnable() {
			@Override
			public void run() {
				SingletonWawita3 singletonWawita9 = SingletonWawita3.getSingletonWawtita();
				System.out.println("singleton 3 : " + singletonWawita9);
			}
		});
	}

}
